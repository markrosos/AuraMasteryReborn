<GuiXml>
	<Controls>

		<!-- Generic Aura Anchor -->
		<TopLevelControl name="AM_GenericAnchor" hidden="true" level="2" clampedToScreen="true">
			<Dimensions x="12" y="12" />
			<Anchor point="TOPLEFT" relativeTo="AuraMasteryTLW" anchorPoint="TOPLEFT" />
			<Controls>
				<Backdrop name="$(parent)_Backdrop" centerColor="FF00FF00" edgeColor="FF000000">
					<AnchorFill />
					<Edge edgeSize="1" /> 
				</Backdrop>
				<Label name="$(parent)_LabelCoords" font="$(BOLD_FONT)|16|soft-shadow-thin" color="00FF00" text="400,400">
					<Anchor point="TOP" relativeTo="$(parent)" anchorPoint="TOP" offsetY="-20" />	
				</Label>
			</Controls>
			<OnMoveStop>
				local offsetX = self:GetLeft()
				local offsetY = self:GetTop()
				local activeAura = AuraMastery.activeAura
				AuraMastery:SaveAuraPosX(activeAura, offsetX)
				AuraMastery:SaveAuraPosY(activeAura, offsetY)
				AuraMasteryDisplayMenu_AuraPosXSubControl_Editbox:SetText(offsetX)
				AuraMasteryDisplayMenu_AuraPosYSubControl_Editbox:SetText(offsetY)
			</OnMoveStop>
		</TopLevelControl>
	
		<TopLevelControl name="AuraMasteryHiddenControls" hidden="true" />	

		<TopLevelControl name="AuraMasteryExportWindow" clampedToScreen="true" mouseEnabled="true" tier="1" layer="2" hidden="true">	<!-- Draw Tier 2 = High, 1 = medium, 0 = low, 999 = max value(?) -->
			<Dimensions x="800" y="450" />
			<Anchor point="CENTER" relativeTo="GuiRoot" relativePoint="CENTER" />
			<Controls>
				<Backdrop name="$(parent)_Backdrop" inherits="ZO_DefaultBackdrop" />
				<Label name="$(parent)_Label" font="$(BOLD_FONT)|18|soft-shadow-thin" color="C5C29E">
					<Anchor point="TOP" relativeTo="$(parent)" anchorPoint="TOP" offsetY="6" />
				</Label>
				<Button name="AuraMasteryExportWindowCloseButton" inherits="ZO_CloseButton">
					<OnClicked>self:GetParent():SetHidden(true)</OnClicked>
				</Button>
				<!-- Editbox Export -->
				<Control name="ExportEditboxCaption">
					<Dimensions x="792" y="414" />
					<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="4" offsetY="32" />
					<Controls>
						<Backdrop name="$(parent)_Backdrop" centerColor="66000000" edgeColor="1F1F1F">
							<AnchorFill />
							<Edge edgeSize="1" />
							<Controls>
								<EditBox name="ExportEditbox" font="$(BOLD_FONT)|14|soft-shadow-thin" mouseEnabled="true" maxInputCharacters="100000" editEnabled="false" multiLine="true">
									<AnchorFill />
									<OnMouseDown>
										AuraMastery:EditBoxOnMouseDown(self)
									</OnMouseDown>
									<OnFocusLost>
										AuraMastery:EditBoxOnFocusLost(self)
									</OnFocusLost>
								</EditBox>
							</Controls>
						</Backdrop>
					</Controls>
				</Control>
				<!-- /Editbox Export -->
			</Controls>
		</TopLevelControl>

		<TopLevelControl name="AuraMasteryImportWindow" clampedToScreen="true" mouseEnabled="true" movable="true" tier="1" layer="2" hidden="true">	<!-- Draw Tier 2 = High, 1 = medium, 0 = low, 999 = max value(?) -->
			<Dimensions x="800" y="450" />
				<Anchor point="CENTER" relativeTo="GuiRoot" relativePoint="CENTER" />
			<Controls>
				<Backdrop name="$(parent)_Backdrop" inherits="ZO_DefaultBackdrop" />
				<Label name="$(parent)_Label" font="$(BOLD_FONT)|18|soft-shadow-thin" color="C5C29E" text="Import aura">
					<Anchor point="TOP" relativeTo="$(parent)" anchorPoint="TOP" offsetY="6" />
				</Label>
				<Button name="AuraMasteryImportWindowCloseButton" inherits="ZO_CloseButton">
					<OnClicked>self:GetParent():SetHidden(true)</OnClicked>
				</Button>
				<Control name="ImportEditboxControl">
					<Dimensions x="792" y="342" />
					<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="4" offsetY="32" />
					<Controls>
						<Backdrop name="$(parent)_Backdrop" centerColor="66000000" edgeColor="1F1F1F">
							<AnchorFill />
							<Edge edgeSize="1" />
							<Controls>
								<EditBox name="ImportEditbox" font="$(BOLD_FONT)|14|soft-shadow-thin" mouseEnabled="true" maxInputCharacters="100000" multiLine="true" newLineEnabled="true">
									<AnchorFill />
									<OnMouseDown>
										AuraMastery:EditBoxOnMouseDown(self)
									</OnMouseDown>
									<OnFocusLost>
										AuraMastery:EditBoxOnFocusLost(self)
									</OnFocusLost>
									<OnMouseEnter>
										AuraMastery:EditBoxOnMouseEnter(self)
									</OnMouseEnter>
									<OnMouseExit>
										AuraMastery:EditBoxOnMouseExit(self)
									</OnMouseExit>
								</EditBox>
							</Controls>
						</Backdrop>
					</Controls>
				</Control>
				<Label name="$(parent)_NotificationLabel" font="$(BOLD_FONT)|16|soft-shadow-thin">
					<Anchor point="BOTTOMLEFT" relativeTo="$(parent)" anchorPoint="BOTTOMLEFT" offsetX="5" offsetY="-39" />
				</Label>
				<Button name="ImportActionButton" inherits="ZO_DefaultButton" text="Import">
					<Dimensions x="292" y="30" />
					<Anchor point="BOTTOM" relativeTo="$(parent)" relativePoint="BOTTOM" offsetY="-3" />
					<OnClicked>
							AuraMastery:ImportAura()
					</OnClicked>
				</Button>
			</Controls>
		</TopLevelControl>

		<TopLevelControl name="AuraMasteryTriggerEventsWindow" clampedToScreen="true" mouseEnabled="true" movable="true" tier="1" layer="2" hidden="true">	<!-- Draw Tier 2 = High, 1 = medium, 0 = low, 999 = max value(?) -->
			<Dimensions x="500" y="450" />
				<Anchor point="CENTER" relativeTo="GuiRoot" relativePoint="CENTER" />
			<Controls>
				<Backdrop name="$(parent)_Backdrop" inherits="ZO_DefaultBackdrop" />
				<Label name="$(parent)_Label" font="$(BOLD_FONT)|18|soft-shadow-thin" color="C5C29E" text="Trigger Events">
					<Anchor point="TOP" relativeTo="$(parent)" anchorPoint="TOP" offsetY="6" />
				</Label>
				<Button name="$(parent)_CloseButton" inherits="ZO_CloseButton">
					<OnClicked>self:GetParent():SetHidden(true)</OnClicked>
				</Button>
				
				<!-- Event Controls are created in lua (auramenu.lua) -->
				
			</Controls>
		</TopLevelControl>

		<TopLevelControl name="AuraMasteryAuraTextWindow" clampedToScreen="true" mouseEnabled="true" movable="true" tier="1" layer="2" hidden="true">	<!-- Draw Tier 2 = High, 1 = medium, 0 = low, 999 = max value(?) -->
			<OnInitialized>
			
				-- initialize position select
				local m_comboBox = AuraTextPositionSubControl.m_comboBox
				m_comboBox:SetSortsItems(false)
				m_comboBox:SetSelectedItemFont("$(BOLD_FONT)|14|soft-shadow-thin")
				local items = {}
				table.insert(items, {name = "topleft", callback = AuraMastery.EditAuraTextPosition, internalValue = 1})
				table.insert(items, {name = "top", callback = AuraMastery.EditAuraTextPosition, internalValue = 2})
				table.insert(items, {name = "topright", callback = AuraMastery.EditAuraTextPosition, internalValue = 3})
				table.insert(items, {name = "left", callback = AuraMastery.EditAuraTextPosition, internalValue = 4})
				table.insert(items, {name = "right", callback = AuraMastery.EditAuraTextPosition, internalValue = 5})
				table.insert(items, {name = "bottomleft", callback = AuraMastery.EditAuraTextPosition, internalValue = 6})
				table.insert(items, {name = "bottom", callback = AuraMastery.EditAuraTextPosition, internalValue = 7})
				table.insert(items, {name = "bottomright", callback = AuraMastery.EditAuraTextPosition, internalValue = 8})
				AuraMastery:PopulateComboBoxCustom(m_comboBox, items, AuraMastery.EditAuraTextPosition)
				
				-- initialize textinfo select
				local m_comboBox = AuraTextInfoSourceSubControl.m_comboBox
				m_comboBox:SetSortsItems(false)
				m_comboBox:SetSelectedItemFont("$(BOLD_FONT)|14|soft-shadow-thin")
			</OnInitialized>
			<Dimensions x="492" y="360" />
			<Anchor point="BOTTOM" relativeTo="GuiRoot" relativePoint="BOTTOM" offsetY="-30" />
			<Controls>
				<Backdrop name="$(parent)_Backdrop" inherits="ZO_DefaultBackdrop" />
				
				<Label name="$(parent)_Label" font="$(BOLD_FONT)|18|soft-shadow-thin" color="C5C29E" text="Aura text">
					<Anchor point="TOP" relativeTo="$(parent)" anchorPoint="TOP" offsetY="6" />
				</Label>
				
				<Button name="$(parent)CloseButton" inherits="ZO_CloseButton">
					<OnClicked>self:GetParent():SetHidden(true)</OnClicked>
				</Button>
				
				<Control name="$(parent)EditBoxContainer">
					<Dimensions x="340" y="200" />
					<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetY="32" />
					<Controls>
						<Backdrop name="$(parent)_Backdrop" centerColor="66000000" edgeColor="1F1F1F">
							<AnchorFill />
							<Edge edgeSize="1" />
							<Controls>
								<EditBox name="AuraMasteryAuraText_TextBox" font="$(BOLD_FONT)|14|soft-shadow-thin" mouseEnabled="true" maxInputCharacters="100000" multiLine="true" newLineEnabled="true">
									<AnchorFill />
									<OnMouseDown>
										AuraMastery:EditBoxOnMouseDown(self)
									</OnMouseDown>
									<OnFocusLost>
										AuraMastery:EditBoxOnFocusLost(self)
									</OnFocusLost>
									<OnMouseEnter>
										AuraMastery:EditBoxOnMouseEnter(self)
									</OnMouseEnter>
									<OnMouseExit>
										AuraMastery:EditBoxOnMouseExit(self)
									</OnMouseExit>
								</EditBox>
							</Controls>
						</Backdrop>
					</Controls>
				</Control>
				
				<Control name="$(parent)LegendContainer" inherits="AM_TriggerControl">
					<OnInitialized>
						local label = self:GetNamedChild("_Label")
						label:SetAnchor(TOPLEFT, self, TOPLEFT, 4, 3)
						label:SetText("Replacer Legend:")
					</OnInitialized>
					<Dimensions x="150" y="200" />
					<Anchor point="TOPRIGHT" relativeTo="$(parent)" relativePoint="TOPRIGHT" offsetY="32" />
					<Controls>
						<Label name="AuraMasteryAuraTextWindowLegendLabel" font="$(BOLD_FONT)|16|soft-shadow-thin">
							<Anchor point="TOPLEFT" relativeTo="$(parent)" anchorPoint="TOPLEFT" offsetX="4" offsetY="10" />
						</Label>
					</Controls>
				</Control>
				
				<Label name="$(parent)_NotificationLabel" font="$(BOLD_FONT)|16|soft-shadow-thin">
					<Anchor point="BOTTOMLEFT" relativeTo="$(parent)" anchorPoint="BOTTOMLEFT" offsetY="-94" />
				</Label>

				<Control name="AuraTextFontSizeContainer" inherits="AM_TriggerControl">
					<OnInitialized>
						self:GetNamedChild("_Label"):SetText("Font size:")
						AuraTextFontSizeSubControl_Editbox:SetHandler("OnEnter", function() AuraMastery.EditAuraTextFontSize(); end)
					</OnInitialized>
					<Dimensions x="210" y="28" />
					<Anchor point="BOTTOMLEFT" relativeTo="$(parent)" relativePoint="BOTTOMLEFT" offsetY="-64" />
					<Controls>
						<Backdrop name="AuraTextFontSizeSubControl" inherits="AM_EditBox">
							<Dimensions x="42" y="20" />
							<Anchor point="RIGHT" relativeTo="$(parent)" relativePoint="RIGHT" offsetX="-4" />
						</Backdrop>
					</Controls>
				</Control>
				
				<Control name="AuraTextFontColorContainer" inherits="AM_TriggerControl">
					<OnInitialized>
						self:GetNamedChild("_Label"):SetText("Font color:")
						self.data = {}
						local iconSelect = LAMCreateControl.colorpicker(self, {
							type = "colorpicker",
							name = "",
							tooltip = "Select font color.",
							getFunc = AuraMastery.GetAuraTextFontColor,
							setFunc = AuraMastery.EditAuraTextFontColor,
							reference = "AuraMasteryDisplayMenu_TextFontColorSelect"
						})
						iconSelect:ClearAnchors()
						iconSelect:SetAnchor(RIGHT, self, RIGHT)
						iconSelect:SetDrawTier(2)
					</OnInitialized>
					<Dimensions x="210" y="28" />
					<Anchor point="BOTTOMLEFT" relativeTo="$(parent)" relativePoint="BOTTOMLEFT" offsetY="-34" />
				</Control>
				
				<Control name="AuraTextPositionContainer" inherits="AM_TriggerControl">
					<OnInitialized>
						self:GetNamedChild("_Label"):SetText("Text position:")
					</OnInitialized>
					<Dimensions x="210" y="28" />
					<Anchor point="BOTTOMLEFT" relativeTo="$(parent)" relativePoint="BOTTOMLEFT" offsetY="-4" />
					<Controls>				
						<Control name="AuraTextPositionSubControl" inherits="AM_ComboBox">
							<Dimensions x="120" y="28" />
							<Anchor point="RIGHT" relativeTo="$(parent)" relativePoint="RIGHT" offsetX="-4" />
						</Control>
					</Controls>
				</Control>

				<Control name="AuraTextInfoSourceContainer" inherits="AM_TriggerControl">
					<OnInitialized>
						self:GetNamedChild("_Label"):SetText("Textinfo Source:")
					</OnInitialized>
					<Dimensions x="210" y="28" />
					<Anchor point="TOPRIGHT" relativeTo="$(parent)" relativePoint="TOPRIGHT" offsetY="236" />
					<Controls>
						<Control name="AuraTextInfoSourceSubControl" inherits="AM_ComboBox">
							<Dimensions x="120" y="28" />
							<Anchor point="RIGHT" relativeTo="$(parent)" relativePoint="RIGHT" offsetX="-4" />
						</Control>
					</Controls>
				</Control>
				
				<Button name="SaveAuraText" inherits="ZO_DefaultButton" text="Save">
					<Dimensions x="140" y="28" />
					<Anchor point="BOTTOMRIGHT" relativeTo="$(parent)" relativePoint="BOTTOMRIGHT" offsetX="-4" offsetY="-4" />
					<OnClicked>
							AuraMastery:EditAuraText()
					</OnClicked>
				</Button>
			</Controls>
		</TopLevelControl>		
				
		<TopLevelControl name="AuraMasteryCustomTriggerActionResultsWindow" clampedToScreen="true" mouseEnabled="true" movable="true" tier="1" layer="2" hidden="true">	<!-- Draw Tier 2 = High, 1 = medium, 0 = low, 999 = max value(?) -->
			<Dimensions x="500" y="450" />
				<Anchor point="CENTER" relativeTo="GuiRoot" relativePoint="CENTER" offsetX="-258" offsetY="232" />
			<Controls>
				<Backdrop name="$(parent)_Backdrop" inherits="ZO_DefaultBackdrop" />
				<Label name="$(parent)_Label" font="$(BOLD_FONT)|18|soft-shadow-thin" color="C5C29E" text="Custom Trigger Events">
